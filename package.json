{
  "name": "huisheng",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "repository": "git@github.com:hans-m-song/huisheng.git",
  "author": "Hans Song <hans.m.song@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "tsc",
    "watch": "npm run compile -- -w",
    "start": "node dist/index.js",
    "generate": "tsx bin/generate-queries.ts schema/queries.sql src/lib/cache/queries.ts",
    "debug": "nodemon",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "prettier --write ./src && yarn lint --fix",
    "test": "jest",
    "test:ci": "jest --runInBand --detectOpenHandles --silent",
    "schema:diff": "atlas schema diff --from sqlite://local.db --to file://schema/tables.sql --dev-url 'sqlite://dev?mode=memory'",
    "schema:apply": "atlas schema apply --url sqlite://local.db --to file://schema/tables.sql --dev-url 'sqlite://dev?mode=memory'"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/static": "^7.0.3",
    "@googleapis/youtube": "^16.1.0",
    "@libsql/client": "^0.6.0",
    "axios": "^0.26.1",
    "bufferutil": "^4.0.8",
    "discord-api-types": "^0.37.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.0.0",
    "erlpack": "github:discord/erlpack",
    "fastify": "^4.26.2",
    "minio": "^7.1.3",
    "newrelic": "^10.6.2",
    "pino": "^8.20.0",
    "preact": "^10.21.0",
    "preact-render-to-string": "^6.4.2",
    "sodium-native": "^4.1.1",
    "source-map-support": "^0.5.21",
    "ts-pattern": "^4.0.1",
    "typescript": "^4.9.5",
    "utf-8-validate": "^5.0.10",
    "zlib-sync": "^0.1.9",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.15",
    "pino-pretty": "^11.0.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.7.0",
    "tsx": "^3.14.0"
  }
}
